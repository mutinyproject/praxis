#!/bin/sh

src_fetch() {
	OLDIFS="${IFS}"; IFS=$'\n'; set -- "${SOURCES}"; IFS="${OLDIFS}"; unset OLDIFS

	while [ $# -gt 0 ]; do
		local output="${FETCHDIR}"/"$(basename "$1")"
		edo curl -# -L -C - -o "${output}".part "$1" && edo mv "${output}".part "${output}"
		shift
	done
}

src_unpack() {
	:
}
